            <p class="number-stat">38</p>
            <p class="desc-stat">Blogs</p>
          </div>
        </div>
        <p class="desc">Hi ! My name is {{logged.username}} {{logged.about}}</p>
        <div>
          <span class="follow">Follow</span>
        </div>
        
        <div class="social">
          <a id="icon"class="fa fa-facebook-square" aria-hidden="true" href="https://facebook.com"></a>
          <a id="icon"class="fa fa-twitter-square" aria-hidden="true"href="https://twitter.com"></a>
        </div>
      </div>
      <div class="right col-lg-8">
        
        
        <div class="row gallery">
          <div class="col-md-4">
             <img src="/images/second.jpeg"onclick="onClick(this)"/>
          </div>
          <div class="col-md-4">
            <img onclick="onClick(this)"src="https://images.pexels.com/photos/2962816/pexels-photo-2962816.jpeg?cs=srgb&dl=black-hoodie-conceptual-creepy-2962816.jpg&fm=jpg"/>
